  "author": "Roman Dmiterko",
  "license": "MIT",
  "scripts": {
    "preinstall": "git config core.hooksPath hooks || echo 'Error configuring hooks path'",
    "dev": "parcel *.html",
    "build": "parcel build *.html --public-url './'",
    "deploy": "yarn build && gh-pages -d dist",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@parcel/resolver-default": "2.13.3",
    "@parcel/transformer-less": "2.13.3",
    "eslint": "9.17.0",
    "gh-pages": "6.2.0",
    "less": "4.2.1",
    "lint-staged": "15.2.11",
    "parcel": "2.13.3",
    "prettier": "3.4.2"
  },
  "resolutions": {
    "@eslint/js": "9.17.0",
    "@parcel/resolver-default": "2.13.3",
    "@parcel/transformer-less": "2.13.3",
    "eslint": "9.17.0",
    "gh-pages": "6.2.0",
    "less": "4.2.1",
    "lint-staged": "15.2.11",
    "parcel": "2.13.3",
    "prettier": "3.4.2"
  },
  "lint-staged": {
    "js/**/*.js": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ],
    "*.{html,css,json}": [
      "yarn prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
